{
  "version": "1.2.1",
  "license": "BSD-3-Clause-Clear",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "cross-env NODE_OPTIONS='--no-deprecation' tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --exit-zero-on-changes"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mat-sz/react-var-ui.git"
  },
  "bugs": {
    "url": "https://github.com/mat-sz/react-var-ui/issues"
  },
  "homepage": "https://github.com/mat-sz/react-var-ui",
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "semi": true,
    "arrowParens": "avoid"
  },
  "name": "react-var-ui",
  "author": "Mat Sz <contact@matsz.dev>",
  "module": "dist/react-var-ui.esm.js",
  "size-limit": [
    {
      "path": "dist/react-var-ui.cjs.production.min.js",
      "limit": "50 KB"
    },
    {
      "path": "dist/react-var-ui.esm.js",
      "limit": "50 KB"
    }
  ],
  "keywords": [
    "react",
    "dat.gui",
    "react-component",
    "react-slider",
    "slider",
    "input",
    "range",
    "library",
    "typescript"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@size-limit/preset-small-lib": "^4.12.0",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addons": "^6.3.8",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.result": "^4.5.6",
    "@types/lodash.set": "^4.3.6",
    "@types/react": "^17.0.11",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^17.0.7",
    "@types/testing-library__jest-dom": "^5.14.1",
    "babel-loader": "^8.2.2",
    "canvas": "^2.8.0",
    "chromatic": "^5.9.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "cssnano": "^5.0.6",
    "husky": "^6.0.0",
    "postcss": "^8.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "sass": "^1.34.1",
    "sass-loader": "^10.1.1",
    "size-limit": "^4.12.0",
    "style-loader": "^2.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "3.9.x"
  },
  "dependencies": {
    "filesize": "^8.0.6",
    "lodash.clonedeep": "^4.0.0",
    "lodash.result": "^4.0.0",
    "lodash.set": "^4.0.0",
    "react-color": "^2.0.0",
    "react-use-pointer-drag": "^0.0.1"
  }
}
